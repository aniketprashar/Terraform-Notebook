{
  "telemetry.telemetryLevel": "off",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Community Material Theme Darker High Contrast",
  "editor.snippetSuggestions": "top",
  "editor.fontSize": 13,
  "editor.lineHeight": 22,
  "editor.fontFamily": "Fira Code",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.DS_Store": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/bower_components": true
  },
  "workbench.colorCustomizations": {
    "editor.background": "#06090e",
    "sideBar.background": "#0e1116",
    "activityBar.background": "#0e1116",
    "statusBar.background": "#06090e",
    "statusBar.noFolderBackground": "#06090e",
    "statusBar.debuggingBackground": "#06090e",
    "tab.inactiveBackground": "#06090e",
    "editorGroupHeader.tabsBackground": "#06090e",
    "breadcrumb.background": "#0e1116",
    "terminal.background": "#0e1116",
    "editor.selectionBackground": "#135564",
    "editor.selectionHighlightBackground": "#135564"
  },
  "editor.trimAutoWhitespace": true,
  "files.trimTrailingWhitespace": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.minimap.enabled": false,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "editor.tokenColorCustomizations": {
    "[Community Material Theme Darker High Contrast]": {
      "comments": "#677073"
    },
    "textMateRules": [
      {
        "name": "No italics",
        "scope": [
          "comment",
          "markup.quote",
          "variable.language",
          "variable.parameter",
          "keyword.control"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "breadcrumbs.enabled": false,
  "comment-divider.length": 130,
  "comment-divider.mainHeaderTransform": "titlecase",
  "comment-divider.subheaderTransform": "titlecase",
  "code-runner.executorMap": {
    "python": "python3"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "leetcode.workspaceFolder": "/Users/aniketprashar/Documents/Workbook/Github Repos/Interview-Notebook/LeetCode",
  "leetcode.editor.shortcuts": ["submit", "test", "description", "solution"],
  "leetcode.hint.commandShortcut": false,
  "leetcode.defaultLanguage": "python3",
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.hint.commentDescription": false,
  "leetcode.showDescription": "Both",
  "leetcode.showLocked": true,
  "explorer.confirmDelete": false
}
